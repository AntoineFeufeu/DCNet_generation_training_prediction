YLIN
YLOG
EOF


rm -f gsac.doc
for j in `cat j`
do
cat ${j}.trf >> gsac.doc
done

ed gsac.doc << EOF
g/\\-/s//\\[en]/gp
w
q
EOF

mv gsac.doc ../../DOC/GSAC.TRF

#####
#	clean up
#####
rm -f j
