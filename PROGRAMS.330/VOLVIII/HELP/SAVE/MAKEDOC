#/bin/sh


cat > j  << EOF
HELP
ABS
ADD
AGC
BANDPASS
BANDREJECT
BEGINGRAPHICS
CD
CHNHDR
COLOR
CORRELATE
CONVOLVE
CUT
CUTERR
DELETE
DIF
DIV
ECHO
ENVELOPE
EXP
EXP10
FFT
FILEID
FILTER
FUNCGEN
HIGHPASS
HILBERT
HOLD
INT
INTERPOLATE
LISTHEADER
LINLIN
LINLOG
LOG
LOG10
LOWPASS
MARKTIMES
MERGE
MUL
PAUSE
PCTL
PLOT1
PLOTPK
PLOTSP
PRS
QDP
READ
RMEAN
ROTATE
ROTATE3
RTREND
SMOOTH
SORT
SQR
SQRT
SUB
SYNCHRONIZE
TRANSFER
WRITE
WHITEN
HISTORY
TAPER
REVERSE
STACK
SGN
WRITEHEADER
XLIM
YLIN
YLOG
EOF


rm -f gsac.doc
for j in `cat j`
do
cat ${j}.trf >> gsac.doc
done

ed gsac.doc << EOF
g/\\-/s//\\[en]/gp
w
q
EOF

mv gsac.doc ../../DOC/GSAC.TRF

#####
#	clean up
#####
rm -f j
